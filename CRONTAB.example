SHELL=/bin/bash
PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin

# Logs cron
*/15 * * * * cd /home/ubuntu/pywikibot-scripts && /usr/bin/flock -n /tmp/muffybot-vandalism-fr.lock /usr/bin/timeout 8m /usr/bin/python3 vandalism.py >> /home/ubuntu/pywikibot-scripts/logs/cron-vandalism-fr.log 2>&1
*/10 * * * * cd /home/ubuntu/pywikibot-scripts && /usr/bin/flock -n /tmp/muffybot-welcome.lock /usr/bin/timeout 9m /usr/bin/python3 welcome.py >> /home/ubuntu/pywikibot-scripts/logs/cron-welcome.log 2>&1
0 * * * * cd /home/ubuntu/pywikibot-scripts && /usr/bin/flock -n /tmp/muffybot-homonym.lock /usr/bin/timeout 55m /usr/bin/python3 homonym.py >> /home/ubuntu/pywikibot-scripts/logs/cron-homonym.log 2>&1
15 3 * * * cd /home/ubuntu/pywikibot-scripts && /usr/bin/flock -n /tmp/muffybot-categinex.lock /usr/bin/timeout 120m /usr/bin/python3 categinex.py >> /home/ubuntu/pywikibot-scripts/logs/cron-categinex.log 2>&1
5 0 * * * cd /home/ubuntu/pywikibot-scripts && /usr/bin/flock -n /tmp/muffybot-daily-report.lock /usr/bin/timeout 20m /usr/bin/python3 daily_report.py >> /home/ubuntu/pywikibot-scripts/logs/cron-daily-report.log 2>&1
20 0 * * * cd /home/ubuntu/pywikibot-scripts && /usr/bin/flock -n /tmp/muffybot-daily-bot-logs.lock /usr/bin/timeout 15m /usr/bin/python3 daily_bot_logs.py >> /home/ubuntu/pywikibot-scripts/logs/cron-daily-bot-logs.log 2>&1
25 0 * * * cd /home/ubuntu/pywikibot-scripts && /usr/bin/flock -n /tmp/muffybot-config-backup.lock /usr/bin/timeout 15m /usr/bin/python3 config_backup.py >> /home/ubuntu/pywikibot-scripts/logs/cron-config-backup.log 2>&1
10 0 * * 1 cd /home/ubuntu/pywikibot-scripts && /usr/bin/flock -n /tmp/muffybot-weekly-report.lock /usr/bin/timeout 30m /usr/bin/python3 weekly_report.py >> /home/ubuntu/pywikibot-scripts/logs/cron-weekly-report.log 2>&1
15 0 1 * * cd /home/ubuntu/pywikibot-scripts && /usr/bin/flock -n /tmp/muffybot-monthly-report.lock /usr/bin/timeout 45m /usr/bin/python3 monthly_report.py >> /home/ubuntu/pywikibot-scripts/logs/cron-monthly-report.log 2>&1
0 6 * * * cd /home/ubuntu/pywikibot-scripts && /usr/bin/flock -n /tmp/muffybot-doctor.lock /usr/bin/timeout 15m /usr/bin/python3 doctor.py >> /home/ubuntu/pywikibot-scripts/logs/cron-doctor.log 2>&1

*/15 * * * * cd /home/ubuntu/pywikibot-scripts && /usr/bin/flock -n /tmp/muffybot-vandalism-en.lock /usr/bin/timeout 8m /usr/bin/python3 envikidia/vandalism.py >> /home/ubuntu/pywikibot-scripts/logs/cron-vandalism-en.log 2>&1
20 0 * * 1 [ $((10#$(date +\%V) % 2)) -eq 0 ] && cd /home/ubuntu/pywikibot-scripts && /usr/bin/flock -n /tmp/muffybot-vandalism-patterns.lock /usr/bin/timeout 25m /usr/bin/python3 vandalism_patterns.py >> /home/ubuntu/pywikibot-scripts/logs/cron-vandalism-patterns.log 2>&1
*/3 * * * * cd /home/ubuntu/pywikibot-scripts && /usr/bin/flock -n /tmp/muffybot-sandboxreset.lock /usr/bin/timeout 150s /usr/bin/python3 envikidia/sandboxreset.py >> /home/ubuntu/pywikibot-scripts/logs/cron-sandboxreset.log 2>&1
5 0 * * 1 cd /home/ubuntu/pywikibot-scripts && /usr/bin/flock -n /tmp/muffybot-semaine.lock /usr/bin/timeout 120m /usr/bin/python3 envikidia/semaine.py >> /home/ubuntu/pywikibot-scripts/logs/cron-semaine.log 2>&1
20 0 1 1 * cd /home/ubuntu/pywikibot-scripts && /usr/bin/flock -n /tmp/muffybot-main.lock /usr/bin/timeout 120m /usr/bin/python3 envikidia/main.py >> /home/ubuntu/pywikibot-scripts/logs/cron-main.log 2>&1
